{
  "compilerOptions": {
    "strict": true,
    "outDir": "dist",
    "target": "es2017",
    "lib": ["es2017", "dom"],
    "jsx": "react-jsx",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "preserveConstEnums": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}